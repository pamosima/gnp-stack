<?xml version="1.0"?>
<!-- Override default user: allow these networks and no password (so curl from 198.18.x and Vector from Docker work). -->
<clickhouse>
  <users>
    <default replace="replace">
      <password></password>
      <networks>
        <ip>::1</ip>
        <ip>127.0.0.1</ip>
        <ip>10.0.0.0/8</ip>
        <ip>192.168.60.0/24</ip>
        <ip>192.168.0.0/16</ip>
        <ip>172.16.0.0/12</ip>
        <ip>198.18.0.0/16</ip>
      </networks>
      <profile>default</profile>
      <quota>default</quota>
    </default>
  </users>
</clickhouse>
